{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"npmScope": "learnwithjason",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/nx-cloud",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"],
				"accessToken": "NTNhMzk3ZmItMWRlNy00Nzk1LTg4MzktNDE5ODM3ZDdkN2Y1fHJlYWQtd3JpdGU="
			}
		}
	},
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"]
		}
	},
	"generators": {
		"@nrwl/react": {
			"application": {
				"style": "css",
				"linter": "eslint",
				"babel": true
			},
			"component": {
				"style": "css"
			},
			"library": {
				"style": "css",
				"linter": "eslint"
			}
		}
	},
	"defaultProject": "site"
}
